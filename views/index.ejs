<body>
  <!--navbar-->
  <nav class="navbar navbar-expand-lg">
    <div class="container-fluid">
      <img src="/static/logo_img.png" alt="" class="logo">
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="#">Home</a>
          </li>
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="#">Menu</a>
          </li>
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="#">Service</a>
          </li>
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="#">Offers</a>
          </li>
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="#">Rewards</a>
          </li>
        </ul>
        <i class="fa-solid fa-magnifying-glass icons fa-lg"></i>
        <i class="fa-solid fa-cart-shopping icons fa-lg"></i>
        <i class="fa-solid fa-user icons fa-lg"></i>
      </div>
    </div>
  </nav>

  <section class="colored-section" id="title">
    <div class="container-fluid">

      <div class="row">

        <div class="texts col-lg-6">
          <p class="big-heading">
          <h1>With new hot food, you can satisfy the carving of your crew.</h1>
          </p>
          <button type="button" class="btn bg-danger download-button">Order now</button>
          <button type="button" class="btn bg-light download-button"> Track Order</button>
        </div>

        <div class="col-lg-6">
          <img class="title-image" src="/static/food.png" alt="">
        </div>
      </div>
    </div>
  </section>


