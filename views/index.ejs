  <section class="white-section" id="features">

    <div class="container-fluid">

      <h2 class="service">Our Services</h2>

      <div class="row">
        <div class="feature-box col-lg-4">
          <img class="image" src="/static/order online.png" alt="">
          <h3 class="feature-title">Easy to order</h3>
        </div>

        <div class="feature-box col-lg-4">
          <img class="image" src="/static/delivery 2.png" alt="">
          <h3 class="feature-title">Fast Delivery</h3>
        </div>

        <div class="feature-box col-lg-4">
          <img class="image" src="/static/best.png" alt="">
          <h3 class="feature-title">Best Quality</h3>
        </div>
      </div>
    </div>
  </section>

